type Query {
	node: NodeQuery
}

type NodeQuery {
	companyHome: Node
	userHome: Node
	sharedHome: Node
	sitesHome: Node
}

type Node {
	nodeRef: ID!
	uuid: ID!
	
	name: String
	title: String
	description: String
	created: Date
	modified: Date
	
	primaryParent: Node
	children: [Node]!
	childrenContains: [Node]!
}

type Date {
	iso: String!
}